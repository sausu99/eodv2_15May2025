<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_margin="@dimen/_8sdp"
    android:backgroundTint="@color/white"
    android:background="@drawable/btn_bglboutline2"
    android:layout_height="@dimen/_110sdp">

    <ImageView
        android:id="@+id/typeic"
        android:layout_width="@dimen/_25sdp"
        android:layout_height="@dimen/_25sdp"
        android:layout_marginStart="@dimen/_8sdp"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginEnd="@dimen/_19sdp"
        android:layout_marginBottom="@dimen/_8sdp"
        app:tint="@color/darkgraywhite"
        android:src="@drawable/ic_home1">

    </ImageView>
  <LinearLayout
      android:id="@+id/detailsll"
      android:orientation="vertical"
      android:layout_toEndOf="@+id/typeic"

      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
      <TextView
          android:id="@+id/addresstypetxt"
          android:textSize="@dimen/_15ssp"
          android:layout_marginTop="@dimen/_10sdp"
          android:textColor="@color/black"
          android:fontFamily="sans-serif-black"
          android:text="@string/string493"
          android:layout_toEndOf="@+id/typeic"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"></TextView>
      <TextView
          android:id="@+id/addressline1txt"
          android:textSize="@dimen/_12ssp"
          android:textColor="@color/darkgray"
          android:fontFamily="sans-serif-black"
          android:text="@string/string494"
          android:maxWidth="@dimen/_200sdp"
          android:ellipsize="end"
          android:layout_toEndOf="@+id/typeic"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"></TextView>
      <TextView
          android:id="@+id/addressline2txt"
          android:textSize="@dimen/_12ssp"
          android:textColor="@color/darkgray"
          android:fontFamily="sans-serif-black"
          android:text="@string/string495"
          android:ellipsize="end"
          android:maxWidth="@dimen/_200sdp"
          android:layout_toEndOf="@+id/typeic"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"></TextView>
      <TextView
          android:id="@+id/citytxt"
          android:textSize="@dimen/_12ssp"
          android:textColor="@color/darkgray"
          android:fontFamily="sans-serif-black"
          android:text="@string/string496"
          android:maxWidth="@dimen/_200sdp"
          android:ellipsize="end"
          android:layout_toEndOf="@+id/typeic"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"></TextView>
      <TextView
      android:id="@+id/statetxt"
      android:textSize="@dimen/_12ssp"
      android:textColor="@color/darkgray"
      android:fontFamily="sans-serif-black"
      android:text="@string/string497"
          android:ellipsize="end"
          android:maxWidth="@dimen/_200sdp"
      android:layout_toEndOf="@+id/typeic"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"></TextView>

  </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_marginBottom="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/edit"
            android:layout_marginEnd="@dimen/_5sdp"
            android:background="@drawable/ic_edit"
            app:tint="@color/darkgraywhite"
            android:layout_width="@dimen/_22sdp"
            android:layout_height="@dimen/_22sdp"></ImageView>
        <ImageView
            android:id="@+id/delete"
            android:backgroundTint="@color/red"
            android:background="@drawable/ic_delete"
            android:layout_width="@dimen/_22sdp"
            android:layout_height="@dimen/_22sdp"></ImageView>

    </LinearLayout>

</RelativeLayout>